                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>
select *
, isNull((select top 1 trnfmt_length from SAL_satrnfmt
where tdpf_orgid=tdpf_orgid
and trnfmt_bank_codeno=tdpf_bank
and trnfmt_table='SABANK'
and trnfmt_field='BANK_BANK_NO'
and trnfmt_source_type='001'),0) as trnfmt_length
from SAL_satdpf 
left outer join SAL_sabank
on bank_orgid=tdpf_orgid
and bank_tdpf_seqno=tdpf_seqno
and bank_seqno = @seqno
left outer join sys_code
on code_sys='004'
and code_type='002'
and code_no=tdpf_bank
where tdpf_orgid=@orgid
order by tdpf_bank, tdpf_seqno </CommandText>
